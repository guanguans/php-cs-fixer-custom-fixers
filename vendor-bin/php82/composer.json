{
    "require-dev": {
        "php": ">=8.2",
        "ergebnis/composer-normalize": "^2.50",
        "guanguans/monorepo-builder-worker": "^3.1",
        "illuminate/support": "^12.50",
        "laravel/pint": "^1.27",
        "mrpunyapal/rector-pest": "^0.1",
        "peckphp/peck": "^0.2",
        "phpro/grumphp-shim": "^2.19",
        "theofidry/composer-inheritance-plugin": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpro/grumphp-shim": false,
            "theofidry/composer-inheritance-plugin": false,
            "wikimedia/composer-merge-plugin": false
        },
        "sort-packages": true
    }
}
